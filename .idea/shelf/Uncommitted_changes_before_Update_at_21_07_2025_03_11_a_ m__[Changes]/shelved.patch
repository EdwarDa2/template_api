+package org.EdwarDa2.repository;
+
+import java.sql.Connection;
+import java.sql.PreparedStatement;
+
+public class SubCategoriaRepository {
+    package org.EdwarDa2.repository;
+
+import org.EdwarDa2.model.Subcategoria;
+import org.EdwarDa2.config.DatabaseConfig;
+
+import java.sql.*;
+import java.util.ArrayList;
+import java.util.List;
+
+    public class SubcategoriaRepository {
+
+        public List<Subcategoria> findByCategoria(int idCategoria) throws SQLException {
+            List<Subcategoria> lista = new ArrayList<>();
+            String sql = "SELECT * FROM subcategoria WHERE id_categoria = ?";
+
+            try (Connection conn = DatabaseConfig.getDataSource().getConnection();
+                 PreparedStatement stmt = conn.prepareStatement(sql)) {
+
+                stmt.setInt(1, idCategoria);
+                ResultSet rs = stmt.executeQuery();
+
+                while (rs.next()) {
+                    Subcategoria s = new Subcategoria();
+                    s.setId_subcategoria(rs.getInt("id_subcategoria"));
+                    s.setNombre_subcategoria(rs.getString("nombre_subcategoria"));
+                    s.setId_categoria(rs.getInt("id_categoria"));
+                    lista.add(s);
+                }
+            }
+            return lista;
+        }
+    }
+
+}
